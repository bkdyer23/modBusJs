{
    "manifest_version": 3,
    "name": "ModBusJs",
    "version": "1.1",
    "content_security_policy": {
        "script-src": ["'self'", "'unsafe-inline'"]
    },
    "action": {
        "default_popup": "index.html",
        "default_icon": "icon.png"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["mainApp.js"]
        }
    ]
}
